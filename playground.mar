fun main(args: Slice[Str]): Never {
  println(2 + 3 + 4)
  if 1 < 2 then println("Whoa")
  for i in 0..5 do println("Hello, {i}!")
  println(3 %!~@^\`/&*+$-<>=. 4)
  print_name[Slice[U8]]()
  exit(0:U8)
}

fun %!~@^\`/&*+$-<>=.(a: U64, b: U64): U64 { 42 }

fun ===[T](a: T, b: T): Never { panic("JS is a horrible language") }

fun print_name[T]() { println(type[T]()) }
